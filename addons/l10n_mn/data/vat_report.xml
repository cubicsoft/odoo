<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>
    <record id="account_report_vat_report" model="account.report">
        <field name="name">VAT Repayment Report</field>
        <field name="root_report_id" ref="account.generic_tax_report"/>
        <field name="country_id" ref="base.mn"/>
        <field name="filter_multi_company">tax_units</field>
        <field name="column_ids">
            <record id="account_report_vat_report_column" model="account.report.column">
                <field name="name">Balance</field>
                <field name="expression_label">balance</field>
            </record>
        </field>
        <field name="line_ids">
            <record id="vat_report_lineA" model="account.report.line">
                <field name="name">А. ТУХАЙН САРЫН БОРЛУУЛСАН БАРАА, АЖИЛ, ҮЙЛЧИЛГЭЭНИЙ НӨАТ-ЫН ТООЦОО</field>
                <field name="code">VAT_LINEA</field>
                <field name="sequence">1000</field>
                <field name="children_ids">
                    <record id="vat_report_line1" model="account.report.line">
                        <field name="name">Тухайн сарын нийт борлуулалтын орлого (1=2+3)</field>
                        <field name="code">VAT_LINE1</field>
                        <field name="sequence">1001</field>
                        <field name="aggregation_formula">VAT_LINE2.balance + VAT_LINE3.balance</field>
                    </record>
                    <record id="vat_report_line2" model="account.report.line">
                        <field name="name">Хуулийн 13-р зүйлд заасан тухайн сарын НӨАТ-аас чөлөөлөгдөх борлуулалт (2)</field>
                        <field name="code">VAT_LINE2</field>
                        <field name="sequence">1002</field>
                        <field name="expression_ids">
	                        <record id="l10n_mn_vat_report_line2_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:02 НӨАТ-аас чөлөөлөгдөх борлуулалт</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line3" model="account.report.line">
                        <field name="name">Нэмэгдсэн өртгийн албан татвар ногдох бараа, ажил, үйлчилгээний нийт борлуулалтын орлого (3=4+10+29)</field>
                        <field name="code">VAT_LINE3</field>
                        <field name="sequence">1003</field>
                        <field name="aggregation_formula">VAT_LINE4.balance + VAT_LINE10.balance + VAT_LINE29.balance</field>
                    </record>
                    <record id="vat_report_line4" model="account.report.line">
                        <field name="name">НӨАТ ногдох бараа борлуулсны орлого (4=5+...+9)</field>
                        <field name="code">VAT_LINE4</field>
                        <field name="sequence">1004</field>
                        <field name="aggregation_formula">VAT_LINE5.balance + VAT_LINE6.balance + VAT_LINE7.balance + VAT_LINE8.balance + VAT_LINE9.balance</field>
                    </record>
                    <record id="vat_report_line5" model="account.report.line">
                        <field name="name">Дотоодын зах зээлд борлуулсан барааны борлуулалтын орлого (5)</field>
                        <field name="code">VAT_LINE5</field>
                        <field name="sequence">1005</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line5_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:05 НӨАТ-тэй борлуулалт, дотоодын зах зээл</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line6" model="account.report.line">
                        <field name="name">Бусад барааны борлуулалтын орлого (6)</field>
                        <field name="code">VAT_LINE6</field>
                        <field name="sequence">1006</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line6_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:06 Бусад борлуулалтын НӨАТ</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line7" model="account.report.line">
                        <field name="name">Эрх борлуулсны орлого (7)</field>
                        <field name="code">VAT_LINE7</field>
                        <field name="sequence">1007</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line7_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:07 Эрх борлуулсан НӨАТ</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line8" model="account.report.line">
                        <field name="name">Татан буугдах үед өөрт үлдээсэн барааны орлого (8)</field>
                        <field name="code">VAT_LINE8</field>
                        <field name="sequence">1008</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line8_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:08 Татан буугдах үед борлуулсан барааны НӨАТ</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line9" model="account.report.line">
                        <field name="name">Өрийн төлбөрт шилжүүлсэн барааны орлого (9)</field>
                        <field name="code">VAT_LINE9</field>
                        <field name="sequence">1009</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line9_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:09 Өрийн төлбөрт шилжүүлсэн барааны НӨАТ</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line10" model="account.report.line">
                        <field name="name">НӨАТ ногдох ажил, үйлчилгээний орлого (11+...+24)</field>
                        <field name="code">VAT_LINE10</field>
                        <field name="sequence">1010</field>
                        <field name="aggregation_formula">VAT_LINE11.balance + VAT_LINE12.balance + VAT_LINE13.balance + VAT_LINE14.balance + VAT_LINE15.balance + VAT_LINE16.balance + VAT_LINE17.balance + VAT_LINE18.balance + VAT_LINE19.balance + VAT_LINE20.balance + VAT_LINE21.balance + VAT_LINE22.balance + VAT_LINE23.balance + VAT_LINE24.balance</field>
                    </record>
                    <record id="vat_report_line11" model="account.report.line">
                        <field name="name">Өрийн төлбөрт тооцсон ажил гүйцэтгэх, үйлчилгээ үзүүлсний орлого (11)</field>
                        <field name="code">VAT_LINE11</field>
                        <field name="sequence">1011</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line11_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:11 Өрийн төлбөрт үзүүлсэн үйлчилгээний НӨАТ</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line12" model="account.report.line">
                        <field name="name">Цахилгаан, дулаан, хий, ус хангамж, ариутгах татуурга, шуудан, харилцаа холбооны болон бусад үйлчилгээ үзүүлсний орлого (12)</field>
                        <field name="code">VAT_LINE12</field>
                        <field name="sequence">1012</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line12_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:12 Цахилгаан, дулаан, ус, шуудан, холбоо, бусад үйлчилгээний НӨАТ</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line13" model="account.report.line">
                        <field name="name">Бараа түрээслүүлэх, бусад хэлбэрээр эзэмшүүлэх, ашиглуулах үйлчилгээний орлого (13)</field>
                        <field name="code">VAT_LINE13</field>
                        <field name="sequence">1013</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line13_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:13 Бараа түрээслүүлэх, эзэмшүүлэх үйлчилгээний НӨАТ</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line14" model="account.report.line">
                        <field name="name">Зочид буудал ба түүнтэй адилтгах байр түрээслүүлэх, эзэмшүүлэх, ашиглуулах үйлчилгээний орлого (14)</field>
                        <field name="code">VAT_LINE14</field>
                        <field name="sequence">1014</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line14_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:14 Зочид буудал, байр түрээслүүлэх, эзэмшүүлэх үйлчилгээний НӨАТ</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line15" model="account.report.line">
                        <field name="name">Үл хөдлөх, хөдлөх эд хөрөнгө түрээслүүлэх ба эзэмшүүлэх ашиглуулах үйлчилгээий орлого (15)</field>
                        <field name="code">VAT_LINE15</field>
                        <field name="sequence">1015</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line15_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:15 Хөдлөх, үл хөдлөх хөрөнгө түрээслүүлэх, эзэмшүүлэх үйлчилгээний НӨАТ</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line16" model="account.report.line">
                        <field name="name">Шинэ бүтээл, бүтээгдэхүүний загвар, зохиогчийн эрхэд хамаарах бүтээл, барааны тэмдэг, ноу-хау шилжүүлсэн, түрээсэлсэн, худалдсаны орлого (16)</field>
                        <field name="code">VAT_LINE16</field>
                        <field name="sequence">1016</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line16_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:16 Бүтээл, загвар, зохиогчийн эрх борлуулсан НӨАТ</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line17" model="account.report.line">
                        <field name="name">Эд мөнгөний хонжворт сугалаа, төлбөрт таавар бооцоот тоглоомын үйл ажиллагааны орлого (17)</field>
                        <field name="code">VAT_LINE17</field>
                        <field name="sequence">1017</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line17_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:17 Хонжворт сугалаа, таавар, бооцоот тоглоомын НӨАТ</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line18" model="account.report.line">
                        <field name="name">Зуучлалын үйлчилгээний орлого (18)</field>
                        <field name="code">VAT_LINE18</field>
                        <field name="sequence">1018</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line18_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:18 Зуучлалын үйлчилгээний НӨАТ</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line19" model="account.report.line">
                        <field name="name">Бусдын буруутай үйл ажиллагааны улмаас авсан хүү, торгууль, алдангийн орлого (19)</field>
                        <field name="code">VAT_LINE19</field>
                        <field name="sequence">1019</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line19_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:19 Авсан хүү торгууль, алдангийн НӨАТ</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line20" model="account.report.line">
                        <field name="name">Хөрөнгийн үнэлгээний үйлчилгээний орлого (20)</field>
                        <field name="code">VAT_LINE20</field>
                        <field name="sequence">1020</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line20_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:20 Хөрөнгийн үнэлгээний үйлчилгээний НӨАТ</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line21" model="account.report.line">
                        <field name="name">Төрөөс олгож буй төсвийн санхүүжилт, татаас, урамшууллын орлого (21)</field>
                        <field name="code">VAT_LINE21</field>
                        <field name="sequence">1021</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line21_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:21 Төсвийн санхүүжилт, татаасын орлогын НӨАТ</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line22" model="account.report.line">
                        <field name="name">Өмгөөлөл, хууль зүйн зөвлөлгөө өгөх үйлчилгээний орлого (22)</field>
                        <field name="code">VAT_LINE22</field>
                        <field name="sequence">1022</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line22_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:22 Хууль зүйн үйлчилгээний НӨАТ</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line23" model="account.report.line">
                        <field name="name">Үсчин, гоо сайхан, засвар үйлчилгээ, угаалга, хими цэвэрлэгээний үйлчилгээний орлого (23)</field>
                        <field name="code">VAT_LINE23</field>
                        <field name="sequence">1023</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line23_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:23 Үсчин, гоо сайхан, угаалга, цэвэрлэгээний НӨАТ</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line24" model="account.report.line">
                        <field name="name">Хуулийн 13 дугаар зүйлд зааснаас бусад бүх төрлийн үйлчилгээний орлого (24)</field>
                        <field name="code">VAT_LINE24</field>
                        <field name="sequence">1024</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line24_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:24 Хуулийн 13-с бусад үйлчилгээний НӨАТ</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line25" model="account.report.line">
                        <field name="name">Тухайн сарын НӨАТ ногдох дотоодын бараа, ажил, үйлчилгээний орлого (25=4+10)</field>
                        <field name="code">VAT_LINE25</field>
                        <field name="sequence">1025</field>
                        <field name="aggregation_formula">VAT_LINE4.balance + VAT_LINE10.balance</field>
                    </record>
                    <record id="vat_report_line26" model="account.report.line">
                        <field name="name">Ногдуулсан татвар (26=25*10%)</field>
                        <field name="code">VAT_LINE26</field>
                        <field name="sequence">1026</field>
                        <field name="aggregation_formula">VAT_LINE25.balance - (VAT_LINE25.balance / 1.10)</field>
                    </record>
                    <record id="vat_report_line27" model="account.report.line">
                        <field name="name">Нэмэгдсэн өртгийн албан татварын 0 хувь хэрэглэх бараа борлуулсны орлого (27)</field>
                        <field name="code">VAT_LINE27</field>
                        <field name="sequence">1027</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line27_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:27 НӨАТ 0 хувьтай бараа борлуулалт</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line28" model="account.report.line">
                        <field name="name">Нэмэгдсэн өртгийн албан татварын 0 хувь хэрэглэх үйлчилгээ үзүүлсний орлого (28)</field>
                        <field name="code">VAT_LINE28</field>
                        <field name="sequence">1028</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line28_expression" model="account.report.expression">
	                            <field name="label">balance</field>
	                            <field name="engine">tax_tags</field>
	                            <field name="formula">ТТ03:28 НӨАТ 0 хувьтай үйлчилгээ борлуулалт</field>
	                        </record>
	                    </field>
                    </record>
                    <record id="vat_report_line29" model="account.report.line">
                        <field name="name">Тухайн сарын НӨАТ ногдох экспортын бараа, ажил, үйлчилгээний орлого (29=27+28)</field>
                        <field name="code">VAT_LINE29</field>
                        <field name="sequence">1029</field>
                        <field name="aggregation_formula">VAT_LINE27.balance + VAT_LINE28.balance</field>
                    </record>
                    <record id="vat_report_line30" model="account.report.line">
                        <field name="name">Ногдуулсан татвар (30=29*0%)</field>
                        <field name="code">VAT_LINE30</field>
                        <field name="sequence">1030</field>
                        <field name="aggregation_formula">(VAT_LINE27.balance + VAT_LINE28.balance) * 0</field>
                    </record>
                    <record id="vat_report_line31" model="account.report.line">
                        <field name="name">Тухайн сард ногдуулсан НӨАТ-ын нийт татвар (31=26+30)</field>
                        <field name="code">VAT_LINE31</field>
                        <field name="sequence">1031</field>
                        <field name="aggregation_formula">VAT_LINE26.balance + VAT_LINE30.balance</field>
                    </record>
                </field>
            </record>
            <record id="vat_report_lineB" model="account.report.line">
                <field name="name">Б. ТУХАЙН САРД ХУДАЛДАН АВСАН БАРАА, АЖИЛ, ҮЙЛЧИЛГЭЭНИЙ НӨАТ-ЫН ТООЦОО</field>
                <field name="code">VAT_LINEB</field>
                <field name="sequence">2000</field>
                <field name="children_ids">
                    <record id="vat_report_line32" model="account.report.line">
                        <field name="name">Тухайн сард худалдан авсан бараа, ажил үйлчилгээний нийт дүн (32)</field>
                        <field name="code">VAT_LINE32</field>
                        <field name="sequence">2032</field>
                        <field name="expression_ids">
                            <record id="l10n_mn_vat_report_line32_expression" model="account.report.expression">
                                <field name="label">balance</field>
                                <field name="engine">tax_tags</field>
                                <field name="formula">ТТ03:32 Нийт худалдан авсан бараа, үйлчилгээний НӨАТ</field>
                            </record>
                        </field>
                    </record>
                    <record id="vat_report_line33" model="account.report.line">
